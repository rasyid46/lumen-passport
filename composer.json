    "description": "Making Laravel Passport work with Lumen",
    "keywords": [
        "php",
        "lumen",
        "laravel passport"
    ],
    "homepage": "https://github.com/dusterio/lumen-passport",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Mysenko",
            "email": "denis@mysenko.com",
            "homepage": "https://www.mysenko.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0",
        "laravel/passport": ">=0.2.2",
        "laminas/laminas-diactoros": "^3.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19|^1.9.1",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Dusterio\\LumenPassport\\": "src/"
